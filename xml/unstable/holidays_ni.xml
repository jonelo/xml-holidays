<holidays country="NI">
    <!-- http://en.wikipedia.org/wiki/Public_holidays_in_Nicaragua -->
    
    <holiday name="Begin Daylight Saving Time (DST)">
        <definition>
            <dst timezone="America/Managua" action="start"/>
        </definition>
    </holiday>
    
    <holiday name="End Daylight Saving Time (DST)">
        <definition>
            <dst timezone="America/Managua" action="end"/>
        </definition>
    </holiday>

</holidays>