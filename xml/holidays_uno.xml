<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE holidays SYSTEM "holidays.dtd">
<!--
    This file is from the free xml-holidays project. See also
    https://sourceforge.net/projects/xml-holidays/
    Thank you for keeping the comment.
-->
<holidays country="UNO">
    <!--
      https://www.un.org/en/sections/observances/international-days/
      https://en.wikipedia.org/wiki/Category:United_Nations_days
     -->

    <default language="en"/>

    <!--
      https://en.wikipedia.org/wiki/International_Holocaust_Remembrance_Day
     -->
    <holiday>
        <description language="en" name="International Holocaust Remembrance Day"/>
        <description language="de" name="Internationaler Tag des Gedenkens an die Opfer des Holocaust"/>
        <description language="fr" name="Journée internationale dédiée à la mémoire des victimes de l'Holocauste"/>
        <description language="es" name="Día Internacional de Conmemoración en Memoria de las Víctimas del Holocausto"/>
        <description language="it" name="Giorno della Memoria"/>
        
        <definition firstyear="2005">
            <fixed month="January" day="27"/>
        </definition>
    </holiday>

    <!--
      http://www.who.int/cancer/world-cancer-day/en/
      https://en.wikipedia.org/wiki/World_Cancer_Day
     -->
    <holiday>
        <description language="en" name="World Cancer Day"/>
        <description language="de" name="Weltkrebstag"/>
        <description language="fr" name="Journée mondiale contre le cancer"/>
        <description language="es" name="Día Mundial contra el Cáncer"/>
        <definition firstyear="2010">
            <fixed month="February" day="4"/>
        </definition>
    </holiday>

    <!--
      https://en.wikipedia.org/wiki/International_Women%27s_Day
      -->
    <holiday>
        <description language="en" name="International Women's Day"/>
        <description language="de" name="Internationaler Frauentag (Weltfrauentag)"/>
        <definition firstyear="1978">
            <fixed month="March" day="8"/>
        </definition>
    </holiday>

    <!--
      https://en.wikipedia.org/wiki/
      -->
    <holiday>
        <description language="en" name="World Water Day"/>
        <description language="de" name="Weltwassertag"/>
        <description language="fr" name="Journée mondiale de l'eau"/>
        <description language="es" name="Día Mundial del Agua"/>
        <description language="it" name="Giornata mondiale dell'acqua"/>
        <definition firstyear="1993">
            <fixed month="March" day="22"/>
        </definition>
    </holiday>


    <!--
      https://en.wikipedia.org/wiki/World_Health_Day
      -->
    <holiday>
        <description language="en" name="World Health Day"/>
        <description language="de" name="Weltgesundheitstag"/>
        <definition firstyear="1948">
            <fixed month="April" day="7"/>
        </definition>
    </holiday>

    <!--
      https://en.wikipedia.org/wiki/World_Book_and_Copyright_Day
      -->
    <holiday>
        <description language="en" name="World Book and Copyright Day"/>
        <definition firstyear="1995">
            <fixed month="April" day="23"/>
        </definition>
    </holiday>

    <!--
      https://en.wikipedia.org/wiki/World_Press_Freedom_Day
    -->
    <holiday>
        <description language="en" name="World Press Freedom Day"/>
        <description language="de" name="Internationaler Tag der Pressefreiheit"/>
        <description language="es" name="Día Mundial de la Libertad de Prensa"/>
        <definition firstyear="1994">
            <fixed month="May" day="3"/>
        </definition>
    </holiday>

    <!--
       https://en.wikipedia.org/wiki/United_Nations
       https://en.wikipedia.org/wiki/International_Day_for_Biological_Diversity
       https://www.cbd.int/
     -->
    <holiday>
        <description language="en" name="International Day for Biological Diversity"/>
        <definition firstyear="1993" lastyear="2000">
            <fixed month="December" day="29"/>
        </definition>
        <definition firstyear="2001">
            <fixed month="May" day="22"/>
        </definition>
    </holiday>

    <!--
      https://en.wikipedia.org/wiki/World_Environment_Day
      -->
    <holiday>
        <description language="en" name="World Environment Day"/>
        <description language="de" name="Weltumwelttag (Tag der Umwelt)"/>
        <definition firstyear="1972">
            <fixed month="June" day="5"/>
        </definition>
    </holiday>


    <!--
      https://www.un.org/en/events/cooperativesday/
      https://en.wikipedia.org/wiki/International_Co-operative_Day
      -->
    <holiday>
        <description language="en" name="International Day of Cooperatives"/>
        <description language="de" name="Internationaler Genossenschaftstag"/>
        <description language="fr" name="Journée internationale des coopératives"/>
        <description language="es" name="Día Internacional de las Cooperativas"/>
        <definition firstyear="1995">
            <rule nth="First" weekday="Saturday" month="July"/>
        </definition>
    </holiday>


    <!--
       https://www.un.org/en/events/statisticsday/
       https://en.wikipedia.org/wiki/World_Statistics_Day
    -->
    <holiday>
        <description language="en" name="World Statistics Day"/>
        <description language="de" name="Weltstatistiktag"/>
        <description language="fr" name="Journée mondiale de la statistique"/>
        <description language="es" name="Día Mundial de la Estadística"/>
        <definition firstyear="2010" frequency="5">
            <fixed month="October" day="20"/>
        </definition>
    </holiday>

    <!--
       https://en.wikipedia.org/wiki/United_Nations_Day
    -->
    <holiday>
         <description language="en" name="United Nations Day"/>
         <description language="de" name="Tag der Vereinten Nationen"/>
         <description language="fr" name="Journée des Nations unies"/>
         <description language="es" name="Día de las Naciones Unidas"/>
         <description language="zh" name="联合国日"/>
         <definition firstyear="1947">
             <fixed month="October" day="24"/>
         </definition>
    </holiday>

    <!--
      https://en.wikipedia.org/wiki/Children%27s_Day
     -->
    <holiday>
        <description language="en" name="Universal Children's Day"/>
        <description language="de" name="Weltkindertag"/>
        <description language="fr" name="Journée de l'enfance"/>
        <description language="es" name="Día Universal del Niño"/>
        <definition firstyear="1954">
            <fixed month="November" day="20"/>
        </definition>
    </holiday>

    <!--
       https://en.wikipedia.org/wiki/World_Toilet_Day
    -->
    <holiday>
        <description language="en" name="World Toilet Day"/>
        <description language="de" name="Welttoilettentag"/>
        <description language="fr" name="Journée mondiale des toilettes"/>
        <description language="es" name="Día Mundial del Retrete"/>
        <definition firstyear="2013">
            <fixed month="November" day="19"/>
        </definition>
    </holiday>

    <!--
      https://en.wikipedia.org/wiki/Africa_Industrialization_Day
    -->
    <holiday>
         <description language="en" name="Africa Industrialization Day"/>
         <definition firstyear="1989">
             <fixed month="November" day="20"/>
         </definition>
    </holiday>

    <!--
       http://en.wikipedia.org/wiki/World_AIDS_Day
    -->
    <holiday>
         <description language="en" name="World AIDS Day"/>
         <description language="de" name="Welt-AIDS-Tag"/>
         <description language="fr" name="Journée mondiale de lutte contre le sida"/>
         <description language="es" name="Día Mundial de la Lucha contra el Sida"/>
         <description language="zh" name="世界艾滋病日"/>
         <definition firstyear="1988">
             <fixed month="December" day="1"/>
         </definition>
    </holiday>

    <!--
       https://en.wikipedia.org/wiki/Human_Rights_Day
    -->
    <holiday>
         <description language="en" name="Human Rights Day"/>
         <description language="de" name="Tag der Menschenrechte"/>
         <definition firstyear="1950">
             <fixed month="December" day="10"/>
         </definition>
    </holiday>

</holidays>
